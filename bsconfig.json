{
  "name": "bucklescript-tea-starter-kit",
  "version": "0.1.0",
  "sources": [
    {
      "dir": "src"
    },
    {
      "dir": "__tests__",
      "type": "dev"
    }
  ],
  "package-specs": [{ "module": "es6", "in-source": true }],
  "suffix": ".bs.js",
  "refmt": 3,
  "bs-dependencies": [
    "bucklescript-tea",
    "tablecloth-bucklescript",
    "@glennsl/bs-jest",
    "@glennsl/bs-json" 
  ],
  "bs-dev-dependencies": ["@glennsl/bs-jest"],
  "bsc-flags": [
    "-bs-super-errors -no-alias-deps",
    "-color",
    "always",
    "-w",
    "-40+6+7+27+32..39+44+45",
    "-w @8"
  ]
}
